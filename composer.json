{
  "name": "softius/resources-resolver",
  "type": "library",
  "description": "Resolve file paths and methods",
  "license": "MIT",
  "keywords": [
    "softius",
    "resources-resolver"
  ],
  "homepage": "https://github.com/softius/resources-resolver",
  "authors": [
      {
          "name": "Iacovos Constantinou",
          "email": "softius@gmail.com",
          "role": "Developer"
      }
  ],
  "require": {
    "php": ">=5.6.0",
    "container-interop/container-interop": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0",
    "league/container": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "Softius\\ResourcesResolver\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Softius\\ResourcesResolver\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": [
      "phpunit"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
